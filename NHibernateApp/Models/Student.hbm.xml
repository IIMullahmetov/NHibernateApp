<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="NHibernateApp" namespace="NHibernateApp.Models">
	<class name="Student" table="Students" dynamic-update="true" xmlns="urn:nhibernate-mapping-2.2">
		<cache usage="read-write"/>
		<id name="Id" column="Id" type="int">
			<generator class="identity"/>
			</id>
		<property name="FirstName"/>
		<property name="MiddleName"/>
		<property name="LastName"/>
		<property name="Email"/>
		<property name="GroupNumber"/>
	</class>
</hibernate-mapping>